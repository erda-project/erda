        "staticData": {},
        "config": {
          "dataSourceConfig": {
            "activedMetricGroups": [
              "all",
              "all@ta_timing"
            ],
            "isSqlMode": false,
            "resultFilters": [
              {
                "alias": "tk",
                "field": "ta_timing-tk::tag",
                "filter": {
                  "operation": "=",
                  "value": "{{tk}}"
                },
                "key": "filterUiWXW5A9",
                "resultType": "string",
                "type": "filter"
              }
            ],
            "sortDimensions": [
              {
                "aggregation": "avg",
                "alias": "plt",
                "field": "ta_timing-plt::field",
                "key": "sort2ugz1GjE",
                "resultType": "number",
                "sort": "DESC",
                "type": "sort"
              }
            ],
            "typeDimensions": [
              {
                "alias": "时间",
                "key": "type06dR78sm",
                "type": "time"
              },
              {
                "alias": "browser",
                "field": "ta_timing-browser::tag",
                "key": "typertS3i5eh",
                "resultType": "string",
                "type": "field"
              }
            ],
            "valueDimensions": [
              {
                "aggregation": "avg",
                "alias": "平均加载时间",
                "field": "ta_timing-plt::field",
                "key": "valueUavVicP1",
                "resultType": "number",
                "type": "field",
                "unit": {
                  "type": "TIME",
                  "unit": "ms"
                }
              }
            ]
          },
          "optionProps": {
            "isMoreThanOneDay": false
          }
        },
        "api": {
          "body": {
            "from": [
              "ta_timing"
            ],
            "groupby": [
              "time()",
              "browser::tag"
            ],
            "orderby": [],
            "select": [
              {
                "alias": "type06dR78sm",
                "expr": "time()"
              },
              {
                "alias": "typertS3i5eh",
                "expr": "browser::tag"
              },
              {
                "alias": "valueUavVicP1",
                "expr": "round_float(avg(plt::field), 2)"
              }
            ],
            "where": [
              "tk::tag='{{tk}}'"
            ]
          },
          "method": "post",
          "query": {
            "end": "{{endTime}}",
            "epoch": "ms",
            "filter__metric_scope": "micro_service",
            "filter__metric_scope_id": "{{tk}}",
            "filter_terminus_key": "{{tk}}",
            "format": "chartv2",
            "ql": "influxql:ast",
            "start": "{{startTime}}",
            "type": "_"
          },
          "url": "/api/tmc/metrics-query"
        }
      }
