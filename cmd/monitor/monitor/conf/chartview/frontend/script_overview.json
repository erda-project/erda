                "sort": "DESC",
                "type": "sort"
              }
            ],
            "typeDimensions": [
              {
                "alias": "时间",
                "key": "typepsbws9VS",
                "type": "time"
              },
              {
                "alias": "error",
                "field": "ta_error-error::tag",
                "key": "typeSH3FiyL2",
                "resultType": "string",
                "type": "field"
              }
            ],
            "valueDimensions": [
              {
