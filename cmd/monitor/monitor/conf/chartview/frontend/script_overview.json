                "type": "field"
              }
            ]
          },
          "optionProps": {
            "isMoreThanOneDay": false
          }
        },
        "api": {
          "body": {
            "from": [
              "ta_error"
            ],
            "groupby": [
              "time()",
              "error::tag"
            ],
            "orderby": [
              {
                "dir": "DESC",
                "expr": "count(count::field)"
              }
            ],
            "select": [
              {
                "alias": "typepsbws9VS",
                "expr": "time()"
              },
              {
                "alias": "typeSH3FiyL2",
                "expr": "error::tag"
              },
              {
                "alias": "valueeQySN6BL",
                "expr": "round_float(count(count::field), 2)"
              }
            ],
            "where": [
              "tk::tag='{{tk}}'"
            ]
          },
          "method": "post",
          "query": {
            "end": "{{endTime}}",
            "epoch": "ms",
            "filter__metric_scope": "micro_service",
            "filter__metric_scope_id": "{{tk}}",
            "filter_terminus_key": "{{tk}}",
            "format": "chartv2",
            "ql": "influxql:ast",
            "start": "{{startTime}}",
            "type": "_"
          },
          "url": "/api/tmc/metrics-query"
        }
