{
  "id":"app_exception_count",
  "expression":{
    "filters":[
      {
        "operator":"eq",
        "tag":"org_name",
        "value":"$org_name"
      },
      {
        "operator":"eq",
        "tag":"terminus_key",
        "value":"$terminus_key"
      }
    ],
    "functions":[
      {
        "aggregator":"sum",
        "field":"count",
        "operator":"gte",
        "value":20
      }
    ],
    "group":[
      "error_id"
    ],
    "metric":"error_alert",
    "outputs":[
      "alert"
    ],
    "select":{
      "application_id":"#application_id",
      "application_name":"#application_name",
      "class":"#class",
      "cluster_name":"#cluster_name",
      "error_id":"#error_id",
      "host_ip":"#host_ip",
      "instance_id":"#instance_id",
      "line":"#line",
      "method":"#method",
      "org_name":"#org_name",
      "project_id":"#project_id",
      "project_name":"#project_name",
      "runtime_id":"#runtime_id",
      "runtime_name":"#runtime_name",
      "service_name":"#service_name",
      "terminus_key":"#terminus_key",
      "type":"#type",
      "workspace":"#workspace"
    },
    "window":5
  }
}
