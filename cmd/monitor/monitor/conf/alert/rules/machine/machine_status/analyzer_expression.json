{
  "id":"machine_status",
  "expression":{
    "filters":[
      {
        "operator":"eq",
        "tag":"org_name",
        "value":"$org_name"
      }
    ],
    "functions":[
      {
        "aggregator":"values",
        "field":"status",
        "operator":"all",
        "value":"not_ready"
      },
      {
        "aggregator":"max",
        "alias":"last_timestamp_value",
        "field":"last_timestamp"
      }
    ],
    "group":[
      "host_ip"
    ],
    "metric":"machine_status",
    "outputs":[
      "alert"
    ],
    "select":{
      "_meta":"#_meta",
      "_metric_scope":"#_metric_scope",
      "_metric_scope_id":"#_metric_scope_id",
      "cluster_name":"#cluster_name",
      "host_ip":"#host_ip",
      "labels":"#labels",
      "org_name":"#org_name"
    },
    "window":5
  }
}
