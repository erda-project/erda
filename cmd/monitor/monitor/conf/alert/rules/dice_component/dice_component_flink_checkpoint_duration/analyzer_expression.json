{
  "id":"dice_component_flink_checkpoint_duration",
  "expression":{
    "filters":[
      {
        "operator":"eq",
        "tag":"org_name",
        "value":"$org_name"
      }
    ],
    "functions":[
      {
        "aggregator":"max",
        "alias":"lastCheckpointDuration_value",
        "field":"lastCheckpointDuration",
        "operator":"gte",
        "value":3000
      }
    ],
    "group":[
      "cluster_name",
      "container_id",
      "job_id"
    ],
    "metric":"flink_jm_job",
    "outputs":[
      "alert"
    ],
    "select":{
      "cluster_name":"#cluster_name",
      "host_ip":"#host_ip",
      "job_id":"#job_id",
      "org_name":"#org_name"
    },
    "window":1
  }
}
