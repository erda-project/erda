{
  "gemini-2.5-flash-lite": {
    "placeholders": [
      {
        "name": "target-model-name",
        "type": "string",
        "required": false,
        "mapping": {
          "by": {
            "service_provider_type": {
              "google-vertex-ai": {
                "by": {
                  "path_matcher": {
                    "/v1/chat/completions": "google/${@template.name}",
                    "default": "${@template.name}"
                  }
                }
              }
            }
          },
          "default": "${@template.name}"
        }
      }
    ],
    "metadata": {
      "recommended_service_provider_templates": [
        "google-vertex-ai"
      ],
      "website": "https://docs.cloud.google.com/vertex-ai/generative-ai/docs/models/gemini/2-5-flash-lite"
    },
    "desc": "Gemini 2.5 Flash-Lite is a lightweight reasoning model in the Gemini 2.5 family, optimized for ultra-low latency and cost efficiency. It offers improved throughput, faster token generation, and better performance across common benchmarks compared to earlier Flash models. By default, \\\"thinking\\\" (i.e. multi-pass reasoning) is disabled to prioritize speed, but developers can enable it via the [Reasoning API parameter](https://openrouter.ai/docs/use-cases/reasoning-tokens) to selectively trade off cost for intelligence.",
    "config": {
      "desc": "${@template.desc}",
      "apiKey": "",
      "type": "text_generation",
      "publisher": "google",
      "metadata": {
        "public": {
          "abilities": {
            "abilities": [
              "tool",
              "reasoning",
              "image_recognition",
              "video_recognition",
              "reasoning",
              "enable_thinking",
              "structured_output"
            ],
            "input_modalities": [
              "text",
              "image",
              "audio",
              "video"
            ],
            "output_modalities": [
              "text"
            ]
          },
          "context": {
            "context_length": 1048576,
            "max_completion_tokens": 65536,
            "max_prompt_tokens": 1048576
          },
          "model_name": "${@template.placeholders.target-model-name}",
          "pricing": {
            "prompt": "0.0000001",
            "completion": "0.0000004",
            "request": "0",
            "image": "0",
            "image_output": "0",
            "web_search": "0",
            "internal_reasoning": "0",
            "input_cache_read": "0.00000001",
            "input_cache_write": "0.0000001833",
            "unit": "USD"
          }
        },
        "secret": {}
      }
    }
  }
}
