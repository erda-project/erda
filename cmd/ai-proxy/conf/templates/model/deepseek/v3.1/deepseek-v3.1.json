{
  "deepseek-v3.1": {
    "placeholders": [
      {
        "name": "target-model-name",
        "type": "string",
        "required": false,
        "default": "${@template.name}"
      }
    ],
    "metadata": {
      "recommended_service_provider_templates": [
        "volcengine-ark",
        "aliyun-bailian"
      ],
      "website": "https://bailian.console.aliyun.com/cn-beijing/?tab=model#/model-market/detail/deepseek-v3.1"
    },
    "desc": "是深度求索全新推出的混合推理模型，支持思考与非思考2种推理模式，较 deepseek-r1-0528 思考效率更高。",
    "config": {
      "desc": "${@template.desc}",
      "apiKey": "",
      "type": "text_generation",
      "publisher": "deepseek",
      "metadata": {
        "public": {
          "abilities": {
            "abilities": [
              "reasoning",
              "tool",
              "enable_thinking"
            ],
            "input_modalities": [
              "text"
            ],
            "output_modalities": [
              "text"
            ]
          },
          "context": {
            "context_length": 128000,
            "max_completion_tokens": 32000,
            "max_prompt_tokens": 96000
          },
          "model_name": "${@template.placeholders.target-model-name}",
          "pricing": {
            "completion": "0.000012",
            "image": "0",
            "input_cache_read": "0.0000008",
            "input_cache_write": "0.000000017",
            "internal_reasoning": "0",
            "prompt": "0.000004",
            "request": "0",
            "unit": "CNY",
            "web_search": "0"
          }
        },
        "secret": {}
      }
    }
  }
}
