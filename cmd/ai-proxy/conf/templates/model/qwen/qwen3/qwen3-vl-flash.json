{
  "qwen3-vl-flash": {
    "placeholders": [
      {
        "name": "target-model-name",
        "type": "string",
        "required": false,
        "default": "${@template.name}"
      }
    ],
    "metadata": {
      "recommended_service_provider_templates": [
        "aliyun-bailian"
      ],
      "website": "https://bailian.console.aliyun.com/?tab=model#/model-market/detail/qwen3-vl-flash"
    },
    "desc": "Qwen3系列小尺寸视觉理解模型，实现思考模式和非思考模式的有效融合，效果优于开源版Qwen3-VL-30B-A3B，响应速度快。全面升级图像/视频理解，支持长视频长文档等超长上下文、空间感知与万物识别；具备视觉2D/3D定位能力，胜任复杂现实任务。",
    "config": {
      "desc": "${@template.desc}",
      "apiKey": "",
      "type": "text_generation",
      "publisher": "qwen",
      "metadata": {
        "public": {
          "abilities": {
            "abilities": [
              "tool",
              "structured_output",
              "image_recognition",
              "video_recognition",
              "reasoning",
              "enable_thinking"
            ],
            "input_modalities": [
              "text",
              "image",
              "video"
            ],
            "output_modalities": [
              "text"
            ]
          },
          "context": {
            "context_length": 256000,
            "max_prompt_tokens": 254000,
            "max_completion_tokens": 32000
          },
          "model_name": "${@template.placeholders.target-model-name}",
          "pricing": {
            "prompt": "0.0000003",
            "completion": "0.000003",
            "image": "0",
            "input_cache_read": "0",
            "input_cache_write": "0",
            "internal_reasoning": "0",
            "request": "0",
            "unit": "CNY",
            "web_search": "0"
          }
        },
        "secret": {}
      }
    }
  }
}
