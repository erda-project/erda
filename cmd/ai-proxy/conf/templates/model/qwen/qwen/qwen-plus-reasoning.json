{
  "qwen-plus-reasoning": {
    "placeholders": [
      {
        "name": "target-model-name",
        "type": "string",
        "required": false,
        "default": "${@template.name}"
      }
    ],
    "metadata": {
      "recommended_service_provider_templates": [
        "aliyun-bailian"
      ],
      "website": "https://bailian.console.aliyun.com/cn-beijing/?tab=model#/model-market/detail/qwen-plus-reasoning"
    },
    "desc": "Qwen-Plus 能力均衡，推理效果、成本和速度介于通义千问-Max和通义千问-Turbo之间，适合中等复杂任务。",
    "config": {
      "desc": "${@template.desc}",
      "apiKey": "",
      "type": "text_generation",
      "publisher": "qwen",
      "metadata": {
        "public": {
          "abilities": {
            "abilities": [
              "tool",
              "reasoning",
              "enable_thinking",
              "structured_output",
              "responses_api"
            ],
            "input_modalities": [
              "text"
            ],
            "output_modalities": [
              "text"
            ]
          },
          "context": {
            "context_length": 131072,
            "max_completion_tokens": 16384,
            "max_prompt_tokens": 114688
          },
          "extra_json_body_for_stream": "{\"enable_thinking\": true}",
          "model_name": "${@template.placeholders.target-model-name}",
          "pricing": {
            "completion": "0.000008",
            "image": "0",
            "input_cache_read": "0",
            "input_cache_write": "0",
            "internal_reasoning": "0.0000008",
            "prompt": "0.0000008",
            "request": "0",
            "unit": "CNY",
            "web_search": "0"
          },
          "request_type": "openai"
        },
        "secret": {}
      }
    }
  }
}
