{
  "gpt-5.2": {
    "placeholders": [
      {
        "name": "target-model-name",
        "type": "string",
        "required": false,
        "default": "${@template.name}"
      }
    ],
    "metadata": {
      "recommended_service_provider_templates": [
        "azure-ai-foundry"
      ],
      "website": "https://platform.openai.com/docs/models/gpt-5.2"
    },
    "desc": "GPT-5.2 is the latest frontier-grade model in the GPT-5 series, offering stronger agentic and long context perfomance compared to GPT-5.1. It uses adaptive reasoning to allocate computation dynamically, responding quickly to simple queries while spending more depth on complex tasks.\\n\\nBuilt for broad task coverage, GPT-5.2 delivers consistent gains across math, coding, sciende, and tool calling workloads, with more coherent long-form answers and improved tool-use reliability.",
    "config": {
      "desc": "${@template.desc}",
      "apiKey": "",
      "type": "text_generation",
      "publisher": "openai",
      "metadata": {
        "public": {
          "abilities": {
            "abilities": [
              "image_recognition",
              "reasoning",
              "responses_api",
              "structured_output",
              "tool"
            ],
            "input_modalities": [
              "text",
              "image"
            ],
            "output_modalities": [
              "text"
            ]
          },
          "api": {
            "apiStyleConfigs": {
              "POST:/v1/chat/completions": {
                "body": {
                  "force": {
                    "temperature": 1
                  },
                  "rename": {
                    "max_tokens": "max_completion_tokens"
                  }
                }
              }
            }
          },
          "context": {
            "context_length": 400000,
            "max_prompt_tokens": 272000,
            "max_completion_tokens": 128000
          },
          "deployment_id": "${@template.placeholders.target-model-name}",
          "model_name": "${@template.placeholders.target-model-name}",
          "pricing": {
            "prompt": "0.00000175",
            "completion": "0.000014",
            "request": "0",
            "image": "0",
            "image_output": "0",
            "web_search": "0.01",
            "internal_reasoning": "0",
            "input_cache_read": "0.000000175",
            "unit": "USD"
          }
        },
        "secret": {}
      }
    }
  }
}
