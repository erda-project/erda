{
  "gpt-4o": {
    "placeholders": [
      {
        "name": "target-model-name",
        "type": "string",
        "required": false,
        "default": "${@template.name}"
      }
    ],
    "metadata": {
      "recommended_service_provider_templates": [
        "azure-ai-foundry"
      ]
    },
    "desc": "GPT-4o 是 OpenAI 推出的最新旗舰多模态大模型，能够以极快速度和高质量处理文本、语音、图像等多种数据，实现实时对话、多语言理解与多模态交互。 ",
    "config": {
      "desc": "${@template.desc}",
      "apiKey": "",
      "type": "text_generation",
      "publisher": "openai",
      "metadata": {
        "public": {
          "abilities": {
            "abilities": [
              "tool",
              "structured_output",
              "reasoning",
              "enable_thinking"
            ],
            "input_modalities": [
              "text"
            ],
            "output_modalities": [
              "text"
            ]
          },
          "context": {
            "context_length": 128000,
            "max_completion_tokens": 16384,
            "max_prompt_tokens": 0
          },
          "deployment_id": "${@template.placeholders.target-model-name}",
          "deployment_name": "${@template.placeholders.target-model-name}",
          "model_name": "${@template.placeholders.target-model-name}",
          "pricing": {
            "completion": "0.00001",
            "image": "0.003613",
            "input_cache_read": "1.25e-06",
            "input_cache_write": "0",
            "internal_reasoning": "0",
            "prompt": "0.0000025",
            "request": "0",
            "unit": "USD",
            "web_search": "0"
          }
        },
        "secret": {}
      }
    }
  }
}
