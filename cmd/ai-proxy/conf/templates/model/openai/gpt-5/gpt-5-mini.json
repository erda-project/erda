{
  "gpt-5-mini": {
    "placeholders": [
      {
        "name": "target-model-name",
        "type": "string",
        "required": false,
        "default": "${@template.name}"
      }
    ],
    "metadata": {
      "recommended_service_provider_templates": [
        "azure-ai-foundry"
      ],
      "website": "https://platform.openai.com/docs/models/gpt-5-mini"
    },
    "desc": "GPT-5 Mini is a compact version of GPT-5, designed to handle lighter-weight reasoning tasks. It provides the same instruction-following and safety-tuning benefits as GPT-5, but with reduced latency and cost. GPT-5 Mini is the successor to OpenAI's o4-mini model.",
    "config": {
      "desc": "${@template.desc}",
      "apiKey": "",
      "type": "text_generation",
      "publisher": "openai",
      "metadata": {
        "public": {
          "abilities": {
            "abilities": [
              "tool",
              "reasoning",
              "image_recognition",
              "responses_api"
            ],
            "input_modalities": [
              "text",
              "image"
            ],
            "output_modalities": [
              "text"
            ]
          },
          "api": {
            "apiStyleConfigs": {
              "POST:/v1/chat/completions": {
                "body": {
                  "force": {
                    "temperature": 1
                  },
                  "rename": {
                    "max_tokens": "max_completion_tokens"
                  }
                }
              }
            }
          },
          "context": {
            "context_length": 400000,
            "max_completion_tokens": 128000,
            "max_prompt_tokens": null
          },
          "deployment_id": "${@template.placeholders.target-model-name}",
          "model_name": "${@template.placeholders.target-model-name}",
          "pricing": {
            "completion": "0.000002",
            "image": "0",
            "input_cache_read": "0.000000025",
            "input_cache_write": "0",
            "internal_reasoning": "0",
            "prompt": "0.00000025",
            "request": "0",
            "unit": "USD",
            "web_search": "0"
          }
        },
        "secret": {}
      }
    }
  }
}
