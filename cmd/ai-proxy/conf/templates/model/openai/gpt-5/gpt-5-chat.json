{
  "gpt-5-chat": {
    "placeholders": [
      {
        "name": "target-model-name",
        "type": "string",
        "required": false,
        "default": "${@template.name}"
      }
    ],
    "metadata": {
      "recommended_service_provider_templates": [
        "azure-ai-foundry"
      ]
    },
    "desc": "GPT-5 Chat is designed for advanced, natural, multimodal, and context-aware conversations for enterprise applications.",
    "config": {
      "desc": "${@template.desc}",
      "apiKey": "",
      "type": "text_generation",
      "publisher": "openai",
      "metadata": {
        "public": {
          "abilities": {
            "abilities": [
              "image_recognition",
              "responses_api",
              "tool"
            ],
            "input_modalities": [
              "text",
              "image"
            ],
            "output_modalities": [
              "text"
            ]
          },
          "api": {
            "apiStyleConfigs": {
              "POST:/v1/chat/completions": {
                "body": {
                  "drop": [
                    "reasoning_effort"
                  ],
                  "force": {
                    "temperature": 1
                  },
                  "rename": {
                    "max_tokens": "max_completion_tokens"
                  }
                }
              }
            }
          },
          "context": {
            "context_length": 400000,
            "max_completion_tokens": 128000,
            "max_prompt_tokens": null
          },
          "deployment_id": "${@template.placeholders.target-model-name}",
          "model_name": "${@template.placeholders.target-model-name}",
          "pricing": {
            "completion": "0.00001",
            "image": "0",
            "input_cache_read": "0.000000125",
            "input_cache_write": "0",
            "internal_reasoning": "0",
            "prompt": "0.00000125",
            "request": "0",
            "unit": "USD",
            "web_search": "0"
          }
        },
        "secret": {}
      }
    }
  }
}
