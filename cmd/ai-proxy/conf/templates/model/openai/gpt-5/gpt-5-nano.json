{
  "gpt-5-nano": {
    "placeholders": [
      {
        "name": "target-model-name",
        "type": "string",
        "required": false,
        "default": "${@template.name}"
      }
    ],
    "metadata": {
      "recommended_service_provider_templates": [
        "azure-ai-foundry"
      ],
      "website": "https://platform.openai.com/docs/models/gpt-5-nano"
    },
    "desc": "GPT-5-Nano is the smallest and fastest variant in the GPT-5 system, optimized for developer tools, rapid interactions, and ultra-low latency environments. While limited in reasoning depth compared to its larger counterparts, it retains key instruction-following and safety features. It is the successor to GPT-4.1-nano and offers a lightweight option for cost-sensitive or real-time applications.",
    "config": {
      "desc": "${@template.desc}",
      "apiKey": "",
      "type": "text_generation",
      "publisher": "openai",
      "metadata": {
        "public": {
          "abilities": {
            "abilities": [
              "image_recognition",
              "reasoning",
              "responses_api",
              "structured_output",
              "tool"
            ],
            "input_modalities": [
              "text",
              "image"
            ],
            "output_modalities": [
              "text"
            ]
          },
          "api": {
            "apiStyleConfigs": {
              "POST:/v1/chat/completions": {
                "body": {
                  "force": {
                    "temperature": 1
                  },
                  "rename": {
                    "max_tokens": "max_completion_tokens"
                  }
                }
              }
            }
          },
          "context": {
            "context_length": 400000,
            "max_completion_tokens": 128000,
            "max_prompt_tokens": 272000
          },
          "deployment_id": "${@template.placeholders.target-model-name}",
          "model_name": "${@template.placeholders.target-model-name}",
          "pricing": {
            "completion": "0.0000004",
            "image": "0",
            "input_cache_read": "0.000000005",
            "input_cache_write": "0",
            "internal_reasoning": "0",
            "prompt": "0.00000005",
            "request": "0",
            "unit": "USD",
            "web_search": "0"
          }
        },
        "secret": {}
      }
    }
  }
}
