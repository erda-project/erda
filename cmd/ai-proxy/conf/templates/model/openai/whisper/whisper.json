{
  "whisper": {
    "placeholders": [
      {
        "name": "target-model-name",
        "type": "string",
        "required": false,
        "default": "${@template.name}"
      }
    ],
    "metadata": {
      "recommended_service_provider_templates": [
        "azure-ai-foundry"
      ]
    },
    "config": {
      "desc": "whisper",
      "apiKey": "",
      "type": "audio",
      "publisher": "openai",
      "metadata": {
        "public": {
          "abilities": {
            "abilities": [
              "audio_transcription",
              "audio_translation"
            ],
            "input_modalities": [
              "audio"
            ],
            "output_modalities": [
              "text"
            ]
          },
          "context": {
            "context_length": 10000,
            "max_completion_tokens": 0,
            "max_prompt_tokens": 0
          },
          "deployment_id": "${@template.placeholders.target-model-name}",
          "deployment_name": "${@template.placeholders.target-model-name}",
          "model_name": "${@template.placeholders.target-model-name}",
          "pricing": {
            "completion": "0",
            "image": "0",
            "input_cache_read": "0",
            "input_cache_write": "0",
            "internal_reasoning": "0",
            "prompt": "0.0000001",
            "request": "0",
            "unit": "USD",
            "web_search": "0"
          }
        },
        "secret": {}
      }
    }
  }
}
