{
  "aliyun-bailian": {
    "placeholders": [
      {
        "name": "api-key",
        "type": "string",
        "required": true
      },
      {
        "name": "another-api-key",
        "type": "string",
        "default": "",
        "required": false
      }
    ],
    "desc": "Aliyun Bailian",
    "config": {
      "type": "${@template.name}",
      "desc": "${@template.desc}",
      "apiKey": "${@template.placeholders.api-key}",
      "metadata": {
        "public": {
          "api": {
            "apiStyle": "openai-compatible",
            "apiStyleConfigs": {
              "*:*": {
                "headers": {
                  "Authorization": [
                    "Set",
                    "Bearer ${@provider.apiKey}"
                  ]
                },
                "host": "dashscope.aliyuncs.com",
                "path": [
                  "replace",
                  "/v1",
                  "/compatible-mode/v1"
                ],
                "scheme": "https"
              },
              "*:/proxy/bailian/*": {
                "headers": {
                  "Authorization": [
                    "Set",
                    "Bearer ${@provider.apiKey}"
                  ]
                },
                "host": "dashscope.aliyuncs.com",
                "path": [
                  "replace",
                  "/proxy/bailian/",
                  "/"
                ],
                "queryParams": {},
                "scheme": "https"
              },
              "*:/v1/audio/speech": {
                "headers": {
                  "Authorization": [
                    "Set",
                    "Bearer ${@provider.apiKey}"
                  ]
                },
                "host": "dashscope.aliyuncs.com",
                "path": [
                  "set",
                  "/api/v1/services/aigc/multimodal-generation/generation"
                ],
                "scheme": "https"
              }
            }
          },
          "location": "cn"
        },
        "secret": {
          "anotherApiKey": "${@template.placeholders.another-api-key}"
        }
      }
    }
  }
}
