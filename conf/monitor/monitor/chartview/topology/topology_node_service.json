{
  "id": "topology_node_service",
  "name": "topology_node_service",
  "desc": "",
  "scope": "micro_service",
  "scopeId": "global",
  "viewConfig": [
    {
      "w": 24,
      "h": 4,
      "x": 0,
      "y": 0,
      "i": "view-micro_service_overview",
      "view": {
        "title": "服务概览",
        "description": "",
        "chartType": "card",
        "dataSourceType": "api",
        "staticData": {
        },
        "config": {
          "optionProps": {
            "isMoreThanOneDay": false
          },
          "dataSourceConfig": {
            "valueDimensions": [
              {
                "alias": "运行中实例",
                "key": "running_instances",
                "resultType": "number",
                "type": "field"
              },
              {
                "alias": "已停止实例",
                "key": "stopped_instances",
                "resultType": "number",
                "type": "field"
              },
              {
                "alias": "错误请求",
                "key": "service_error_req_count",
                "resultType": "number",
                "type": "field"
              },
              {
                "alias": "服务异常",
                "key": "service_exception_count",
                "resultType": "number",
                "type": "field"
              },
              {
                "alias": "告警次数",
                "key": "alert_count",
                "resultType": "number",
                "type": "field"
              }
            ]
          }
        },
        "api": {
          "url": "/api/apm/topology/service/overview",
          "query": {
            "scope": "micro_service",
            "terminusKey": "{{terminusKey}}",
            "start": "{{startTime}}",
            "end": "{{endTime}}",
            "serviceName": "{{serviceName}}",
            "serviceId": "{{serviceId}}"
          },
          "method": "get"
        }
      }
    },
    {
      "w": 24,
      "h": 8,
      "x": 0,
      "y": 9,
      "i": "view-service-request-translation",
      "view": {
        "title": "事务分析",
        "description": "",
        "chartType": "table",
        "dataSourceType": "api",
        "staticData": null,
        "config": {
          "optionProps": {
            "invalidToZero": true,
            "isMoreThanOneDay": true,
            "moreThanOneDayFormat": "M/D"
          },
          "dataSourceConfig": {
            "valueDimensions": [
              {
                "aggregation": "sum",
                "alias": "事务类型",
                "key": "requestType",
                "resultType": "string",
                "type": "field"
              },
              {
                "aggregation": "sum",
                "alias": "调用次数",
                "key": "requestCount",
                "resultType": "number",
                "type": "field"
              },
              {
                "aggregation": "avg",
                "alias": "平均响应时间（ms）",
                "key": "requestAvgTime",
                "resultType": "number",
                "type": "field"
              },
              {
                "alias": "错误率 (%)",
                "key": "requestErrorRate",
                "resultType": "number",
                "type": "field"
              }
            ]
          }
        },
        "api": {
          "url": "/api/apm/topology/service/requests",
          "query": {
            "scope": "micro_service",
            "terminusKey": "{{terminusKey}}",
            "start": "{{startTime}}",
            "end": "{{endTime}}",
            "serviceName": "{{serviceName}}",
            "serviceId": "{{serviceId}}"
          },
          "method": "get"
        }
      }
    },
    {
      "w": 24,
      "h": 8,
      "x": 0,
      "y": 18,
      "i": "view-q9ybiy3z",
      "view": {
        "title": "吞吐量",
        "description": "",
        "chartType": "chart:line",
        "dataSourceType": "api",
        "staticData": {
        },
        "config": {
          "optionProps": {
            "invalidToZero": true,
            "isMoreThanOneDay": true,
            "noAreaColor": true,
            "isConnectNulls": true,
            "nullDisplay": "--"
          },
          "dataSourceConfig": {
            "activedMetricGroups": [
              "application_http"
            ],
            "resultFilters": [
              {
                "alias": "目标服务名称",
                "field": "application_http-target_service_name::tag",
                "filter": {
                  "operation": "=",
                  "value": "{{serviceName}}"
                },
                "key": "filterRpvruLuG",
                "resultType": "string",
                "type": "filter"
              }
            ],
            "typeDimensions": [
              {
                "alias": "时间",
                "key": "typeRvHzWW3T",
                "type": "time"
              }
            ],
            "valueDimensions": [
              {
                "aggregation": "sum",
                "alias": "HTTP 请求次数",
                "field": "application_http-elapsed_count::field",
                "key": "valueoTl2ugqu",
                "resultType": "number",
                "type": "field"
              }
            ]
          }
        },
        "api": {
          "url": "/api/tmc/metrics-query",
          "query": {
            "epoch": "ms",
            "filter__metric_scope": "micro_service",
            "filter__metric_scope_id": "{{terminusKey}}",
            "filter_target_application_id": "{{applicationId}}",
            "format": "chartv2",
            "start": "{{startTime}}",
            "end": "{{endTime}}",
            "ql": "influxql:ast",
            "type": "_"
          },
          "body": {
            "from": [
              "application_http"
            ],
            "groupby": [
              "time()"
            ],
            "select": [
              {
                "alias": "typeRvHzWW3T",
                "expr": "time()"
              },
              {
                "alias": "valueoTl2ugqu",
                "expr": "round_float(sum(elapsed_count::field), 2)"
              }
            ],
            "where": [
              "target_service_name::tag='{{serviceName}}'"
            ]
          },
          "header": null,
          "extraData": null,
          "method": "post"
        },
        "controls": null
      }
    }
  ],
  "version": "v2"
}