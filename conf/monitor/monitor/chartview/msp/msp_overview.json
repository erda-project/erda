                "key": "filterb105rFvK",
                "resultType": "string",
                "type": "filter"
              }
            ],
            "typeDimensions": [
              {
                "alias": "时间",
                "key": "typeQXtBniA6",
                "type": "time"
              },
              {
                "alias": "域名",
                "field": "req_host::tag",
                "key": "reqHost",
                "resultType": "string",
                "type": "field"
              }
            ],
            "valueDimensions": [
              {
                "aggregation": "sum",
                "alias": "请求次数",
                "key": "requestCount",
                "resultType": "number",
                "type": "field"
              }
            ]
          }
        },
        "api": {
          "url": "/api/msp/metrics/tenant/project/overview",
          "query": {
            "epoch": "ms",
            "format": "chartv2",
            "ql": "influxql:ast",
            "start": "{{startTime}}",
            "project_key": "gateway",
            "end": "{{endTime}}",
            "type": "_"
          },
          "body": {
            "from": [
              "application_http_service"
            ],
            "groupby": [
              "time()",
              "req_host::tag"
            ],
            "select": [
              {
                "alias": "typeQXtBniA6",
                "expr": "time()"
              },
              {
                "alias": "reqHost",
                "expr": "req_host::tag"
              },
              {
                "alias": "requestCount",
                "expr": "round_float(sum(count_sum::field), 2)"
              }
            ],
            "where": [
              "target_addon_type::tag='ApiGateway'"
            ]
          },
          "header": null,
          "extraData": null,
          "method": "post"
        },
        "controls": null
