        "chartType": "table",
        "dataSourceType": "api",
        "staticData": null,
        "config": {
          "dataSourceConfig": {
            "activedMetricGroups": [
              "application_http_service"
            ],
            "limit": 5,
            "valueDimensions": [
              {
                "alias": "服务名称",
                "key": "serviceName",
                "resultType": "number",
                "type": "field"
              },
              {
                "alias": "所属项目",
                "key": "projectName",
                "resultType": "number",
                "type": "field"
              },
              {
                "alias": "请求次数",
                "key": "countSum",
                "resultType": "number",
                "type": "field"
              }
            ]
          }
        },
        "api": {
          "url": "/api/msp/metrics/tenant/project/overview",
          "query": {
            "epoch": "ms",
            "start": "{{startTime}}",
            "end": "{{endTime}}",
            "project_key": "target_project_id",
            "format": "chartv2",
            "ql": "influxql:ast",
            "type": "_"
          },
          "body": {
            "from": [
              "application_http_service"
            ],
            "groupby": [
              "target_service_name::tag",
              "target_project_id::tag"
            ],
            "limit": 5,
            "orderby": [
              {
                "dir": "DESC",
                "expr": "sum(count_sum::field)"
              }
            ],
            "select": [
              {
                "alias": "serviceId",
                "expr": "target_service_id::tag"
              },
              {
                "alias": "projectId",
                "expr": "target_project_id::tag"
              },
              {
                "alias": "serviceName",
                "expr": "target_service_name::tag"
              },
              {
                "alias": "projectName",
                "expr": "target_project_name::tag"
              },
              {
                "alias": "countSum",
                "expr": "round_float(sum(count_sum::field), 2)"
              }
            ]
          },
          "header": null,
          "extraData": null,
          "method": "post"
        },
        "controls": null
      }
