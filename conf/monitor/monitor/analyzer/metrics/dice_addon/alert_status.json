{
  "attributes": {},
  "version": "3.0",
  "expression": {
    "metric": "analyzer-alert",
    "window": 8,
    "windowBehavior": "allowedLateness",
    "filters": [
      {
        "tag": "alert_type",
        "operator":"eq",
        "value": "dice_addon"
      }
    ],
    "group": [
      "cluster_name",
      "addon_id"
    ],
    "functions": [
      {
        "field": "__level",
        "field_script": "function invoke(field,tag){if(tag.level===\"FATAL\"){return 2}return 1}",
        "aggregator": "max",
        "alias": "health_status"
      },
      {
        "field": "__weight",
        "aggregator": "value",
        "field_script": "function invoke(field,tag){return 0}",
        "alias": "weight"
      }
    ],
    "select": {
      "_meta": "#_meta",
      "_metric_scope": "#_metric_scope",
      "_metric_scope_id": "#_metric_scope_id",
      "component_name": "#addon_id",
      "cluster_name": "#cluster_name",
      "org_name": "#org_name",
      "org_id": "#dice_org_id",
      "component_group": "dice_addon",
      "message": "#alert_name"
    },
    "alias": "leaf_component_status",
    "outputs": [
      "metric"
    ]
  }
}