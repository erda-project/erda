{
  "id":"dice_component_flink_throughput",
  "expression":{
    "filters":[
      {
        "operator":"eq",
        "tag":"org_name",
        "value":"$org_name"
      }
    ],
    "functions":[
      {
        "aggregator":"max",
        "alias":"numRecordsOutPerSecond_count_value",
        "field":"numRecordsOutPerSecond_count",
        "operator":"eq",
        "value":0
      }
    ],
    "group":[
      "cluster_name",
      "container_id",
      "job_id",
      "task_id",
      "subtask_index",
      "operator_id"
    ],
    "metric":"flink_tm_operator",
    "outputs":[
      "alert"
    ],
    "select":{
      "cluster_name":"#cluster_name",
      "host_ip":"#host_ip",
      "job_id":"#job_id",
      "operator_id":"#operator_id",
      "org_name":"#org_name",
      "subtask_index":"#subtask_index",
      "tm_id":"#tm_id"
    },
    "window":1
  }
}
