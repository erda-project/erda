{
  "id":"app_resource_jvm_gc_count",
  "expression":{
    "filters":[
      {
        "operator":"eq",
        "tag":"name",
        "value":"concurrentmarksweep"
      },
      {
        "operator":"eq",
        "tag":"terminus_key",
        "value":"$terminus_key"
      },
      {
        "operator":"eq",
        "tag":"org_name",
        "value":"$org_name"
      }
    ],
    "functions":[
      {
        "aggregator":"sum",
        "field":"count",
        "operator":"gte",
        "value":5
      }
    ],
    "group":[
      "instance_id"
    ],
    "metric":"jvm_gc",
    "outputs":[
      "alert"
    ],
    "select":{
      "application_id":"#application_id",
      "application_name":"#application_name",
      "cluster_name":"#cluster_name",
      "host_ip":"#host_ip",
      "instance_id":"#instance_id",
      "org_name":"#org_name",
      "project_id":"#project_id",
      "project_name":"#project_name",
      "runtime_id":"#runtime_id",
      "runtime_name":"#runtime_name",
      "service_name":"#service_name",
      "terminus_key":"#terminus_key",
      "workspace":"#workspace"
    },
    "window":3
  }
}
