{
  "id":"dice_component_log_protector",
  "expression":{
    "filters":[
      {
        "operator":"eq",
        "tag":"org_name",
        "value":"$org_name"
      },
      {
        "operator":"eq",
        "tag":"field",
        "value":"log_store_protector_triggered"
      }
    ],
    "functions":[
      {
        "aggregator":"value",
        "field":"triggered",
        "operator":"eq",
        "value":1
      }
    ],
    "group":[
      "cluster_name"
    ],
    "metric":"dice_monitor",
    "outputs":[
      "alert"
    ],
    "select":{
      "cluster_name":"#cluster_name",
      "cur_level":"#cur_level",
      "org_name":"#org_name",
      "pre_level":"#pre_level"
    },
    "window":1
  },
  "attributes":{
    "alert_group":"{{cluster_name}}",
    "level":"Notice",
    "recover":"false",
    "tickets_metric_key":"{{cluster_name}}"
  }
}
