{
  "name": "脚本错误",
  "alert_scope": "micro_service",
  "attributes": {
    "level": "Notice",
    "recover": "false",
    "alert_group": "{{tk}}-{{doc_path}}-{{host}}",
    "display_url_id": "micro_bi_script"
  },
  "template": {
    "metric": "ta_error",
    "window": 1,
    "filters": [
      {
        "tag": "tk",
        "operator": "eq",
        "value": "$tk"
      },
      {
        "tag": "type",
        "operator": "eq",
        "value": "browser"
      }
    ],
    "group": [
      "tk",
      "doc_path",
      "host"
    ],
    "functions": [
      {
        "field": "count",
        "aggregator": "sum",
        "operator": "gte",
        "value": 10
      }
    ],
    "select": {
      "url": "#url",
      "type": "#type",
      "tk": "#tk",
      "source": "#source",
      "count": "#count",
      "browser": "#browser",
      "doc_path": "#doc_path",
      "host": "#host",
      "ip": "#ip"
    },
    "outputs": [
      "alert"
    ]
  }
}
