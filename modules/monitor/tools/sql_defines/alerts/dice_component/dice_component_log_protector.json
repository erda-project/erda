{
  "name": "平台组价日志存储保护器状态变化",
  "alert_scope": "org",
  "attributes": {
    "level": "INFO",
    "recover": "false",
    "alert_group": "{{cluster_name}}",
    "tickets_metric_key": "{{cluster_name}}"
  },
  "template": {
    "metric": "dice_monitor",
    "window": 1,
    "filters": [
      {
        "tag": "cluster_name",
        "operator": "in",
        "value": [
          "$cluster_name"
        ]
      },
      {
        "tag": "field",
        "operator": "eq",
        "value": "log_store_protector_triggered"
      }
    ],
    "group": [
      "cluster_name"
    ],
    "functions": [
      {
        "aggregator": "value",
        "field": "triggered",
        "operator": "eq",
        "value": 1
      }
    ],
    "select": {
      "cluster_name": "#cluster_name",
      "pre_level": "#pre_level",
      "cur_level": "#cur_level"
    },
    "outputs": [
      "alert"
    ]
  }
}