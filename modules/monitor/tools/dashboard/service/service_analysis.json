  "viewConfig": [
    {
      "w": 24,
      "h": 12,
      "x": 0,
      "y": 0,
      "i": "view-yJsgVtEN",
      "view": {
        "title": "服务实例",
        "description": "",
        "chartType": "table",
        "dataSourceType": "api",
        "staticData": {
        },
        "config": {
          "optionProps": {
            "isMoreThanOneDay": false,
            "moreThanOneDayFormat": "YYYY/MM/DD",
            "noAreaColor": true
          },
          "dataSourceConfig": {
            "activedMetricGroups": [
              "application_service_node"
            ],
            "limit": 50,
            "valueDimensions": [
              {
                "alias": "实例 ID",
                "key": "valuenCoyVAOd",
                "resultType": "string",
                "type": "field"
              },
              {
                "alias": "运行时版本",
                "key": "valueBfA8yIls",
                "resultType": "string",
                "type": "field"
              },
              {
                "alias": "启动时间",
                "key": "valueSJDzMoXa",
                "resultType": "number",
                "type": "field"
              },
              {
                "alias": "最后心跳时间",
                "key": "last_time",
                "resultType": "number",
                "type": "field"
              },
              {
                "alias": "状态",
                "key": "valuestatus",
                "resultType": "string",
                "type": "field"
              }
            ]
          }
        },
        "api": {
          "url": "/api/tmc/metrics-query",
          "query": {
            "filter__metric_scope": "micro_service",
            "filter__metric_scope_id": "{{terminusKey}}",
            "filter_terminus_key": "{{terminusKey}}",
            "filter_service_id": "{{serviceId}}",
            "filter_service_name": "{{serviceName}}",
            "start": "{{startTime}}",
            "end": "{{endTime}}",
            "format": "chartv2",
            "ql": "influxql:ast",
            "type": "_"
          },
          "body": {
            "from": [
              "application_service_node"
            ],
            "groupby": [
              "service_instance_id::tag"
            ],
            "orderby": [
              {
                "dir": "DESC",
                "expr": "max(timestamp)"
              }
            ],
            "limit": 50,
            "select": [
              {
                "alias": "valuenCoyVAOd",
                "expr": "service_instance_id::tag"
              },
              {
                "alias": "valueBfA8yIls",
                "expr": "service_agent_platform::tag"
              },
              {
                "alias": "valueSJDzMoXa",
                "expr": "format_time(start_time_mean::field*1000000,'2006-01-02 15:04:05')"
              },
              {
                "alias": "last_time",
                "expr": "format_time(timestamp,'2006-01-02 15:04:05')"
              },
              {
                "alias": "valuestatus",
                "expr": "if(gt(now()-timestamp,300000000000),'已停止','运行中')"
              }
            ]
          },
          "header": null,
          "extraData": null,
          "method": "post"
        },
        "controls": null
      }
    },
