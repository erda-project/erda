{
  "attributes": {},
  "version": "3.0",
  "expression": {
    "metric": "analyzer-alert",
    "window": 8,
    "windowBehavior": "allowedLateness",
    "filters": [
      {
        "tag": "alert_type",
        "operator": "eq",
        "value": "machine"
      }
    ],
    "group": [
      "cluster_name",
      "host_ip"
    ],
    "functions": [
      {
        "field": "__level",
        "field_script": "function invoke(field,tag){if(tag.level===\"FATAL\"){return 2}return 1}",
        "aggregator": "max",
        "alias": "health_status"
      },
      {
        "field": "__weight",
        "aggregator": "value",
        "field_script": "function invoke(field,tag){if(!tag.labels){return 0}if(tag.labels.indexOf(\"lb\")!==-1||tag.labels.indexOf(\"master\")!==-1){return 1}return 0}",
        "alias": "weight"
      }
    ],
    "select": {
      "_meta": "#_meta",
      "_metric_scope": "#_metric_scope",
      "_metric_scope_id": "#_metric_scope_id",
      "component_name": "#host_ip",
      "cluster_name": "#cluster_name",
      "org_name": "#_metric_scope_id",
      "component_group": "machine",
      "message": "#alert_name"
    },
    "alias": "leaf_component_status",
    "outputs": [
      "metric"
    ]
  }
}